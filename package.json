    "front:preview": "vite preview",
    "back:dev": "nodemon ./packages/backend/index.ts",
    "back:build": "unbuild",
    "back:start": "node ./dist/backend/index.mjs"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
